<%= simple_form_for(@bodega) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name, as: :string, label: "Nombre de Bodega" %>
    <%= f.input :location, as: :string, label: "Ubicacion" %>
    <%= f.input :description, label: "Descripcion" %>
  </div>

  <div class="form-actions pull-right">
    <%= f.button :submit %>
  </div>
<% end %>
